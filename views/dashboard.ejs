<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= User.name %>
<ul class="list-unstyled">
    <% key.map((pic) => {
      %>
        <li><img src="<%=pic%>" width="500" height="300" class="img-thumbnail" alt="no image"></li>
        <br>
      <%
    }) %>
  </ul>
<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i>Upload</h1>
      <form action="/file/upload" enctype="multipart/form-data" method="POST">
        <div class="form-group">
          File :
          <input
            type="file"
            name="image"
            class="form-control"
          />
        </div>
        <button type="submit" class="btn btn-primary btn-block">Upload</button>
      </form>
      <p class="lead mt-4">
        <a href="/users/logout" class="btn btn-secondary">Logout</a>
        <a href="/file-compress" class="btn btn-secondary">Compress</a>
      </p>
    </div>
  </div>
</div>
</p>
